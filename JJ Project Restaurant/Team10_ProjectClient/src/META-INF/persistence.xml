<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="Team10_ProjectClientPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>team10_projectclient.MicroMarket</class>
    <class>team10_projectclient.DiscountCode</class>
    <class>team10_projectclient.Customer</class>
    <class>team10_projectclient.view.viewreservations.entity.CreditcardTable</class>
    <class>team10_projectclient.view.viewreservations.entity.AdministratorTable</class>
    <class>team10_projectclient.view.viewreservations.entity.FronthouseTable</class>
    <class>team10_projectclient.view.viewreservations.entity.RestaurantTable</class>
    <class>team10_projectclient.view.viewreservations.entity.TableTable</class>
    <class>team10_projectclient.view.viewreservations.entity.WaiterTable</class>
    <class>team10_projectclient.view.viewreservations.entity.FeedbackTable</class>
    <class>team10_projectclient.view.viewreservations.entity.CustomerTable</class>
    <class>team10_projectclient.view.viewreservations.entity.ReservationTable</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/sample"/>
      <property name="javax.persistence.jdbc.user" value="app"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="app"/>
    </properties>
  </persistence-unit>
</persistence>
